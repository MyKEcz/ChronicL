<?xml version="1.0" encoding="utf-8"?>
<customization_form>
  <category name="system">
    <module name="locale">
      <function>
        <set name="single">
          <item name="total_list">en_RU;en_UA;ru_RU;ru_UA;</item>
          <item type="boolean" name="en_RU">yes</item>
          <item type="boolean" name="en_UA">yes</item>
          <item type="boolean" name="ru_RU">yes</item>
          <item type="boolean" name="ru_UA">yes</item>
        </set>
      </function>
    </module>
    <module name="deviceData">
      <function>
        <set name="single">
          <item name="device_name">HTC Legend</item>
        </set>
      </function>
    </module>
    <module name="deviceData1">
      <function>
        <set name="single">
          <item name="sw_number">NA</item>
        </set>
      </function>
    </module>
    <module name="Settings">
      <function name="screen_timeout">
        <set name="plenty">
          <item name="value1">15000</item>
          <item name="value2">30000</item>
          <item name="value3">60000</item>
          <item name="value4">120000</item>
          <item name="value5">600000</item>
          <item name="value6">1800000</item>
        </set>
      </function>
    </module>
    <module name="InputMethod">
      <function name="IME_language_set">
        <set name="single">
          <item type="boolean" name="English">yes</item>
          <item type="boolean" name="Russian">yes</item>
        </set>
      </function>
    </module>
  </category>
  <category name="application">
    <module name="Footprints">
      <function name="settings">
        <set name="settings">
          <item name="retrieve_address">true</item>
          <item name="show_location_disasble_alert">true</item>
          <item name="show_connection_failed_alert">true</item>
          <item name="serial_number">0</item>
          <item name="list_sort">create_time desc</item>
          <item name="camera_white_balance">0</item>
          <item name="camera_brightness">5</item>
        </set>
      </function>
      <function name="kmz">
        <set name="single">
          <item name="kmz_path">/system/customize/resource/preset.kmz</item>
        </set>
      </function>
      <function name="preset">
        <set name="plenty" max="13">
          <item name="name">Лавра</item>
          <item name="create_date">1239629156966</item>
          <item name="category">favorite</item>
          <item name="rate">1</item>
          <item name="accuracy">3</item>
          <item name="latitude">50434983</item>
          <item name="longitude">30556486</item>
          <item name="addr">Київ, Сiчневого Повстання вул., 21</item>
          <item name="comment">Древнейший и один из самых значительных монастырей на Руси. Был основан в 1051 году.</item>
          <item name="pic_filename">/system/customize/resource/Ukraine_cutomization_1st_position_pic.png</item>
        </set>
      </function>
    </module>
    <module name="Crash_Report">
      <function name="setting">
        <set name="single">
          <item name="turn_off_report" type="boolean">no</item>
        </set>
      </function>
    </module>
    <module name="HtcPhone">
      <function name="specificDialNumbers">
        <set name="single">
          <item name="numbers">01,02,03,04</item>
        </set>
      </function>
    </module>
    <module name="OOBE">
      <function name="Block_No_SIM">
        <set name="single">
          <item name="Default" type="boolean">no</item>
        </set>
      </function>
      <function name="Step_Config">
        <set name="single">
          <item name="Set_Language" type="boolean">yes</item>
          <item name="SIP_Tutorial" type="boolean">yes</item>
          <item name="Wi-Fi" type="boolean">yes</item>
          <item name="Data_Sync" type="boolean">no</item>
          <item name="Account_Setup" type="boolean">yes</item>
          <item name="Social_Network_Setup" type="boolean">yes</item>
          <item name="Tutorial" type="boolean">yes</item>
        </set>
      </function>
      <function name="Data_Sync_Config">
        <set name="single">
          <item name="Default" type="boolean">yes</item>
        </set>
      </function>
      <function name="Account_Config">
        <set name="single">
          <item name="Orange_Mail" type="boolean">no</item>
          <item name="Exchange" type="boolean">yes</item>
          <item name="GMail" type="boolean">yes</item>
          <item name="Other_Mail" type="boolean">yes</item>
        </set>
      </function>
      <function name="Social_Network_Config">
        <set name="single">
          <item name="Facebook" type="boolean">yes</item>
          <item name="Flickr" type="boolean">yes</item>
          <item name="Twitter" type="boolean">yes</item>
          <item name="Plurk" type="boolean">yes</item>
        </set>
      </function>
      <function name="Default_Home_Config">
        <set name="single">
          <item name="Set_HTC_Home" type="boolean">yes</item>
        </set>
      </function>
      <function name="Mobile_Data_Config">
        <set name="single">
          <item name="Default" type="boolean">no</item>
        </set>
      </function>
    </module>
  </category>
  <BootConfiguration>
    <BootAnimation animation="/system/customize/resource/bootanimation.zip" audio="/system/customize/resource/android_audio.mp3" />
  </BootConfiguration>
</customization_form>