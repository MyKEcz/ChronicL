          <item name="Set_HTC_Home" type="boolean">yes</item>
        </set>
      </function>
    </module>
    <module name="Footprints">
      <function name="settings">
        <set name="settings">
          <item name="retrieve_address">true</item>
          <item name="show_location_disasble_alert">true</item>
          <item name="show_connection_failed_alert">true</item>
          <item name="serial_number">0</item>
          <item name="list_sort">create_time desc</item>
          <item name="camera_white_balance">0</item>
          <item name="camera_brightness">5</item>
        </set>
      </function>
      <function name="kmz">
        <set name="single">
          <item name="kmz_path">/system/customize/resource/preset.kmz</item>
        </set>
      </function>
      <function name="preset">
        <set name="plenty" max="13">
          <item name="name">Big Ben</item>
          <item name="create_date">1239629156966</item>
          <item name="category">favorite</item>
          <item name="rate">3</item>
          <item name="accuracy">3</item>
          <item name="latitude">51500605</item>
          <item name="longitude">-124608</item>
          <item name="addr">House of Commons, Westminster, London, UK</item>
          <item name="comment">Hearing Big Ben was amazing – everyone's hoping the great weather keeps up.</item>
          <item name="pic_filename">/system/customize/resource/BigBen.jpg</item>
        </set>
        <set name="plenty" max="13">
          <item name="name">Eiffel Tower</item>
          <item name="create_date">1239629156965</item>
          <item name="category">favorite</item>
          <item name="rate">3</item>
          <item name="accuracy">3</item>
          <item name="latitude">48858300</item>
          <item name="longitude">2294500</item>
          <item name="addr">Champ de Mars, 75008 Paris, France</item>
          <item name="site">http://www.tour-eiffel.fr/</item>
          <item name="comment">Drinking wine in the shadow of the Eiffel tower was the best. I couldn't get over how tall it was.</item>
          <item name="pic_filename">/system/customize/resource/EiffelTower.jpg</item>
        </set>
        <set name="plenty" max="13">
          <item name="name">Colosseum</item>
          <item name="create_date">1239629156964</item>
          <item name="category">favorite</item>
          <item name="rate">3</item>
          <item name="accuracy">3</item>
          <item name="latitude">41890169</item>
          <item name="longitude">12492269</item>
          <item name="addr">Piazza del Colosseo, 00184 Rome, Italy</item>
          <item name="comment">On the second day in Rome we got to visit the Colosseum. You could almost hear the sounds of the gladiators.</item>
          <item name="pic_filename">/system/customize/resource/Colosseum.jpg</item>
        </set>
        <set name="plenty" max="13">
          <item name="name">Statue of Liberty</item>
          <item name="create_date">1239629156963</item>
          <item name="category">favorite</item>
          <item name="rate">3</item>
          <item name="accuracy">3</item>
          <item name="latitude">40689200</item>
          <item name="longitude">-74044500</item>
          <item name="addr">National Park Services Liberty Island New York, NY 10004, USA</item>
          <item name="site">http://www.nps.gov/stli/</item>
          <item name="comment">I was really impressed with her beauty. For a second I thought she winked at me.</item>
          <item name="pic_filename">/system/customize/resource/StatueOfLiberty.jpg</item>
        </set>
      </function>
